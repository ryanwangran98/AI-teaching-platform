{
  "name": "ai-teaching-platform-backend",
  "version": "1.0.0",
  "description": "AI融合教学平台后端系统",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon",
    "dev:ts": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node src/server.js",
    "start:prod": "node dist/server.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "ts-node src/scripts/seed.ts",
    "db:seed:new": "ts-node src/scripts/new-seed.ts",
    "db:reset": "prisma migrate reset --force",
    "db:reset:seed": "prisma migrate reset --force && npm run db:seed",
    "db:reset:seed:new": "prisma migrate reset --force && npm run db:seed:new",
    "lint": "eslint src/**/*.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "docker:build": "docker build -t ai-teaching-backend .",
    "docker:run": "docker run -p 3001:3001 --env-file .env ai-teaching-backend"
  },
  "prisma": {
    "seed": "ts-node prisma/complete-seed.ts"
  },
  "keywords": [
    "education",
    "ai",
    "backend",
    "nodejs",
    "express"
  ],
  "author": "AI Teaching Platform Team",
  "license": "MIT",
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@prisma/client": "^5.22.0",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "fluent-ffmpeg": "^2.1.3",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.17.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.4",
    "@types/nodemailer": "^6.4.14",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/winston": "^2.4.4",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
